{{- rawAnchorHeader .Level
    (printf "%s <small>(<a href=\"%s\">Source</a>)</small>"
        (escape .Name)
        (codeHref .Location)
    )
    .Anchor
-}}

{{- spacer -}}

{{- codeBlock "go" .Signature -}}
{{- spacer -}}

{{- template "doc" .Doc -}}

{{- if len .Examples -}}
	{{- spacer -}}

	{{- range (iter .Examples) -}}
		{{- template "example" .Entry -}}
		{{- if (not .Last) -}}{{- spacer -}}{{- end -}}
	{{- end -}}
{{- end -}}

{{- spacer -}}